<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="CFG">
        <class>com.example.jweb_m_l101.entity.User</class>
      <properties>
          <property name="jakarta.persistence.jdbc.url" value="jdbc:sqlserver://localhost:1433;database=JNWEBML101_SMS"/>
          <property name="jakarta.persistence.jdbc.user" value="sa"/>
          <property name="jakarta.persistence.jdbc.password" value="sa"/>
          <property name="jakarta.persistence.jdbc.driver" value="com.microsoft.sqlserver.jdbc.SQLServerDriver"/>
          <property name="dialect" value="org.hibernate.dialect.SQLServerDialect"/>
          <property name="jakarta.persistence.schema-generation.database.action" value="none"/>
      </properties>
  </persistence-unit>
</persistence>
